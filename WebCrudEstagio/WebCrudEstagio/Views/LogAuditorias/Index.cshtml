@model IEnumerable<WebCrudEstagio.Models.LogAuditoria>

@{
    ViewData["Title"] = "Logs de Auditoria";
}

<h1>Logs de Auditoria</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DetalhesAuditoria)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EmailUsuario)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DetalhesAuditoria)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmailUsuario)
            </td>
        </tr>
}
    </tbody>
</table>
